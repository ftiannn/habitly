{
  "name": "habitly-backend",
  "version": "1.0.0",
  "description": "Serverless backend for Habitly habit tracking app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "package:prod": "serverless package --stage prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests --watchAll=false",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:update-snapshots": "jest --updateSnapshot",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:validate": "prisma validate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed/seed-dev.ts",
    "prisma:seed:dev": "tsx prisma/seed/seed-dev.ts",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .serverless"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.817.0",
    "@aws-sdk/client-ssm": "^3.826.0",
    "@prisma/client": "^5.7.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "google-auth-library": "^9.4.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-sdk-client-mock": "^4.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1",
    "jest": "^30.0.0",
    "jest-environment-node": "^30.0.0-beta.3",
    "prisma": "^5.7.0",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-offline": "^13.9.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
